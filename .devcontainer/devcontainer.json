{
  "name": "tuma-task-backend",
  "build": {
    "dockerfile": "../backend/Dockerfile",
    "context": "../backend"
  },
  "features": {},
  "forwardPorts": [8000, 5432, 6379],
  "remoteUser": "vscode",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "ms-python.vscode-pylance",
        "Prisma.prisma",
        "ms-azuretools.vscode-docker"
      ],
      "tasks": {
        "setup": "Run Tasks: Dev Env Setup"
      }
    }
  },
  "postCreateCommand": "bash .devcontainer/scripts/devcontainer_setup.sh"

}
